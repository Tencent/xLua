	"\t.byte 0x8b\n\t.uleb128 0x9\n"	/* offset r11 */
	"\t.byte 0x8c\n\t.uleb128 0x8\n"	/* offset r12 */
	"\t.byte 0x8d\n\t.uleb128 0x7\n"	/* offset r13 */
