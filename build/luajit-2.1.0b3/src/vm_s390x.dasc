  |.macro coroutine_resume_wrap, resume
  |.if resume
  |.ffunc_1 coroutine_resume
  |  lg L:RB, 0(BASE)
  |  lgr L:TMPR0, L:RB			// Save type for checktptp.
  |  cleartp L:RB
